<div class="header  header--sticky">
  <header>
    <div class="header__container container-ds clearfix blocks-container d-flex align-items-center">
      <div class="header__logo float-left logo" role="banner">
        <a href="#hero" class="d-block image-block-inside">
          {% pictureSvgPng "src/assets/img/logo.svg", "To home", "152", "32" %}
        </a>
      </div>
      <div class="header__main-menu-burger burger"></div>

      <div class="header__main-menu-container  float-left" id="main-menu-container">
        <nav aria-label="solbunny main menu">
          <div class="main-menu__button-container">
          </div>
          <ul class="main-menu" role="menubar" aria-label="solbunny main menu">
          {% for link, href in navList %}
            <li class="main-menu__item" role="none">
              <a class="main-menu__link
              {% if(href == page.url) %} main-menu__link--current {% endif %}" 
              {% if(href == page.url) %} aria-current="page" {% endif %}
              href="{{ href }}">{{ link }} </a>
            </li>
          {% endfor %}
          </ul>
        </nav>
      </div>
    </div>
  </header>
</div>